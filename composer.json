{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/common": "^2.10",
        "doctrine/dbal": "^2.9",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "jbroadway/urlify": "^1.1",
        "makinacorpus/calista-core": "^1.0@alpha",
        "makinacorpus/filechunk-bundle": "^2.0.0-alpha8",
        "makinacorpus/goat": "^1.0",
        "makinacorpus/goat-query": "^1.0.0-alpha11",
        "neitanod/forceutf8": "^2.0",
        "ocramius/generated-hydrator": "^3.0",
        "ramsey/uuid": "^3.8",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/event-dispatcher": "5.0.*",
        "symfony/flex": "^1.1",
        "symfony/form": "5.0.*",
        "symfony/framework-bundle": "5.0.*",
        "symfony/http-foundation": "5.0.*",
        "symfony/http-kernel": "5.0.*",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "5.0.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/twig-bundle": "5.0.*",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "5.0.*"
    },
    "config": {
        "platform": {
            "php": "7.3.4"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Goat\\Hydrator\\" : "vendor/makinacorpus/goat/src/Hydrator/",
            "GeneratedHydrator\\Bridge\\Symfony\\": "vendor/makinacorpus/generated-hydrator-bundle/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    },
    "require-dev": {
        "jdorn/sql-formatter": "^1.2"
    }
}
