{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/common": "^2.10",
        "doctrine/dbal": "^2.9",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "jbroadway/urlify": "^1.1",
        "makinacorpus/calista-core": "^1.0@alpha",
        "makinacorpus/filechunk-bundle": "^1.0@beta",
        "makinacorpus/goat-bundle": "^1.0@alpha",
        "makinacorpus/goat-hydrator": "^1.0@alpha",
        "makinacorpus/goat-query": "^1.0.0-alpha11",
        "makinacorpus/php-layout": "dev-master",
        "neitanod/forceutf8": "^2.0",
        "ramsey/uuid": "^3.8",
        "symfony/console": "4.2.*",
        "symfony/dotenv": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.2.*",
        "symfony/framework-bundle": "4.2.*",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "4.2.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/twig-bundle": "4.2.*",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "4.2.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    },
    "require-dev": {
        "jdorn/sql-formatter": "^1.2"
    }
}
